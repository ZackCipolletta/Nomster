<div class="row">
	<% @places.each do |place| %>

	<div class="booyah-boxv col-xs-10 col-xs-offset-1">
		<h1><%= place.name %></h1>
		<i><%= place.address %></i><br /><br />
		<p><%= place.description %></p><br />

		<% if place.user.present? %>
		<small><%= place.user.email %></small>
		<% end %>
	</div>

	<% end %>
</div>
<div class="row">
	<%= paginate @places %>
</div>